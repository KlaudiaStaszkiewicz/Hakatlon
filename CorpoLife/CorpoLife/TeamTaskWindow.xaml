<Window x:Class="CorpoLife.TeamTaskWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CorpoLife"
        mc:Ignorable="d"
        Title="TeamTaskWindow" Height="450" Width="800">
    <Grid>
        <TextBox HorizontalContentAlignment="Center" Height="23" Margin="95,85,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" TextChanged="TextBox_TextChanged" FontWeight="Bold" Foreground="#FF656565" SelectionBrush="{x:Null}" BorderBrush="{x:Null}" Background="{x:Null}"/>

    </Grid>
</Window>
